        {
          "id": 301721,
          "key": "4cc13714-37d7-4705-a714-314ede5290b5",
          "title": "Backpropagation",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "4cc13714-37d7-4705-a714-314ede5290b5",
            "completed_at": null,
            "last_viewed_at": null,
